alias ls='ls --color=auto -oAF --no-group --group-directories-first'
alias ll='ls -l'
alias la='ls -A'
alias l='ls -CF'

alias grep='grep --color=auto'
alias wflist='iwlist wlp2s0 scan | grep -e "ESSID"'
